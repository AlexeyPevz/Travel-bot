# База данных PostgreSQL
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/travel_db

# Telegram
TELEGRAM_TOKEN=YOUR_TELEGRAM_BOT_TOKEN_HERE

# AI
OPENROUTER_API_KEY=your-openrouter-api-key-here

# Level.Travel
LEVELTRAVEL_API_KEY=your-leveltravel-api-key-here
LEVEL_TRAVEL_PARTNER=your-partner-id
LEVEL_TRAVEL_MARKER=your-marker-id
LEVEL_TRAVEL_AFFILIATE_URL=https://your-affiliate-url.com

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=your-redis-password-here

# Security (для development)
JWT_ACCESS_SECRET=generate-strong-secret-min-32-chars
JWT_REFRESH_SECRET=generate-strong-refresh-secret-min-32-chars
JWT_ACCESS_EXPIRY=15m
JWT_REFRESH_EXPIRY=7d
JWT_ISSUER=ai-travel-agent
JWT_AUDIENCE=ai-travel-agent-api
CSRF_SECRET=generate-strong-csrf-secret-min-32-chars
COOKIE_SECRET=generate-strong-cookie-secret-min-32-chars
SESSION_SECRET=generate-strong-session-secret-min-32-chars

# App
APP_URL=http://localhost:5000
NODE_ENV=development
LOG_LEVEL=info