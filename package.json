{
  "name": "ai-travel-agent",
  "version": "1.0.0",
  "description": "AI Travel Agent Telegram Bot with Mini App",
  "main": "server/index.ts",
  "type": "module",
  "scripts": {
    "dev": "tsx watch server/index.ts",
    "build:client": "vite build",
    "build:server": "tsup server/index.ts server/**/*.ts db/migrate.ts --format esm --sourcemap --out-dir dist --clean",
    "build": "npm run build:client && npm run build:server",
    "start": "node dist/server/index.js",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "node dist/db/migrate.js",
    "db:migrate:create": "drizzle-kit generate",
    "db:migrate:drop": "drizzle-kit drop",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx db/seed.ts",
    "test": "jest --config jest.config.cjs",
    "test:watch": "jest --config jest.config.cjs --watch",
    "test:coverage": "jest --config jest.config.cjs --coverage",
    "test:integration": "jest --config jest.integration.config.js",
    "test:integration:watch": "jest --config jest.integration.config.js --watch",
    "test:integration:coverage": "jest --config jest.integration.config.js --coverage",
    "test:all": "npm run test && npm run test:integration",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "prepare": "husky"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@neondatabase/serverless": "^0.9.5",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.14",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@tanstack/react-query": "^5.51.23",
    "@types/cls-hooked": "^4.3.9",
    "@types/cookie-parser": "^1.4.9",
    "@types/express-session": "^1.18.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^10.0.0",
    "axios": "^1.11.0",
    "bull": "^4.11.5",
    "class-variance-authority": "^0.7.1",
    "cls-hooked": "^4.2.2",
    "cookie-parser": "^1.4.7",
    "csrf-csrf": "^4.0.3",
    "date-fns": "^3.6.0",
    "dotenv-safe": "^9.1.0",
    "drizzle-orm": "^0.33.0",
    "express": "^4.19.2",
