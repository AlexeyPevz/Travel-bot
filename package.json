    "react-hook-form": "^7.62.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tailwind-merge": "^2.6.0",
    "uuid": "^11.1.0",
    "vite": "^5.4.2",
